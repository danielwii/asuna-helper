{
  "name": "@danielwii/asuna-helper",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "scripts": {
    "rename": "find dist -depth -name \"*.js\" -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ \\{\\} \\; && find dist -depth -name \"*.js.map\" -exec sh -c 'mv \"$1\" \"${1%.js.map}.mjs.map\"' _ \\{\\} \\;",
    "clean": "rm -rf dist tsconfig.tsbuildinfo .tsbuildinfo",
    "build:watch": "yarn clean && tsc -w",
    "build": "yarn clean && tsc --emitDeclarationOnly --declarationDir dist && swc src -d dist -C module.type=es6 && yarn rename && swc src -d dist -C module.type=commonjs -s",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9",
    "@nestjs/core": ">=9",
    "@nestjs/platform-express": ">=9",
    "redis": ">=5",
    "typeorm": ">=0"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "axios": "^1.2.3",
    "bluebird": "^3.7.2",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "classnames": "^2.3.2",
    "common-tags": "^1.8.2",
    "consola": "^2.15.3",
    "date-fns": "^2.29.3",
    "fs-extra": "^11.1.0",
    "ioredis": "^5.2.5",
    "is-mobile": "^3.1.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nanoid": "^4.0.0",
    "node-buffs": "^0.8.21",
    "ow": "^1.1.0",
    "react-use": "^17.4.0",
    "redis": "^4.5.1",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.11",
    "ua-parser-js": "^1.0.32"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@trivago/prettier-plugin-sort-imports": "git+https://github.com/danielwii/prettier-plugin-sort-imports.git#main",
    "@tsconfig/node18-strictest-esm": "^1",
    "@types/bluebird": "^3",
    "@types/bull": "^4",
    "@types/common-tags": "^1",
    "@types/fs-extra": "^9",
    "@types/lodash": "^4",
    "@types/node": "^18",
    "@types/ua-parser-js": "^0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.0"
}
