{
  "name": "@danielwii/asuna-helper",
  "version": "0.0.9",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "scripts": {
    "rename": "find dist -depth -name \"*.js\" -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ \\{\\} \\; && find dist -depth -name \"*.js.map\" -exec sh -c 'mv \"$1\" \"${1%.js.map}.mjs.map\"' _ \\{\\} \\;",
    "clean": "rm -rf dist tsconfig.tsbuildinfo .tsbuildinfo",
    "build:watch": "yarn clean && tsc -w",
    "build": "yarn clean && tsc --emitDeclarationOnly --declarationDir dist && swc src -d dist -C module.type=es6 && yarn rename && swc src -d dist -C module.type=commonjs -s",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "redis": "^3.1.2",
    "typeorm": "^0.2.38"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.2",
    "@nestjs/platform-express": "^8.1.2",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "bull": "^3.29.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "classnames": "^2.3.1",
    "common-tags": "^1.8.0",
    "consola": "^2.15.3",
    "date-fns": "^2.25.0",
    "fs-extra": "^10.0.0",
    "ismobilejs": "^1.1.1",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "node-buffs": "^0.8.17",
    "ow": "^0.28.1",
    "query-string": "^7.0.1",
    "react-use": "^17.3.1",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "shortid": "^2.2.16",
    "typeorm": "^0.2.38",
    "ua-parser-js": "^0.7.30"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.103",
    "@trivago/prettier-plugin-sort-imports": "git+https://github.com/danielwii/prettier-plugin-sort-imports.git#main",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bluebird": "^3",
    "@types/bull": "^3",
    "@types/common-tags": "^1",
    "@types/fs-extra": "^9",
    "@types/lodash": "^4",
    "@types/node": "^14",
    "@types/redis": "^2",
    "@types/redlock": "^4",
    "@types/shortid": "^0",
    "@types/ua-parser-js": "^0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "packageManager": "yarn@3.0.2",
  "volta": {
    "node": "16.12.0"
  }
}
