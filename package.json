    "class-validator": "^0.13.1",
    "classnames": "^2.3.1",
    "common-tags": "^1.8.0",
    "consola": "^2.15.3",
    "date-fns": "^2.23.0",
    "fs-extra": "^10.0.0",
    "ismobilejs": "^1.1.1",
    "json5": "^2.2.0",
