    "@types/fs-extra": "^9",
    "@types/lodash": "^4",
    "@types/node": "^14",
    "@types/redis": "^2",
    "@types/redlock": "^4",
    "@types/shortid": "^0",
    "@types/ua-parser-js": "^0",
